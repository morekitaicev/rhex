<model name="uneven_terrain">
    <pose>8 0 0  0 0 0</pose>
    <static>true</static>
    <link name="uneven terrain">
        <visual name="visual">
            <geometry>
                <mesh>
                    <uri>file://mountain2.dae</uri>
                    <scale> 3 5 1 </scale>
                </mesh>
            </geometry>
        </visual>
        <collision name="collision">
            <geometry>
                <mesh>
                    <uri>file://mountain2.dae</uri>
                    <scale> 3 5 1 </scale>
                </mesh>
            </geometry>
        </collision>
